# Demo identity
DEMO_WALLET_ADDRESS=0xDEMO_WALLET_ADDRESS
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=

# 0G Storage
# Set to "real" to use 0G Storage SDK upload/download, otherwise mock local storage is used.
# In real mode, publish responses include on-chain upload proof (root hash + transaction hash).
ZERO_G_STORAGE_MODE=mock
ZERO_G_STORAGE_INDEXER_RPC=https://indexer-storage-testnet-turbo.0g.ai

# 0G Compute
# Set to "real" to use 0G Compute broker, otherwise mock responses are returned.
ZERO_G_COMPUTE_MODE=mock
ZERO_G_COMPUTE_PROVIDER=
ZERO_G_COMPUTE_MODEL=qwen/qwen-2.5-7b-instruct

# Shared blockchain configuration for 0G SDKs
ZERO_G_EVM_RPC=https://evmrpc-testnet.0g.ai
ZERO_G_PRIVATE_KEY=

# Payments webhook (optional). If set, /api/webhooks/payments requires matching x-webhook-secret header.
PAYMENTS_WEBHOOK_SECRET=
